   "source": [
    "# Create the below pattern using nested for loop in Python."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\r\n",
      "*\r\n",
      "**\r\n",
      "***\r\n",
      "****\r\n",
      "*****\r\n",
      "****\r\n",
      "***\r\n",
      "**\r\n",
      "*\r\n",
      "\r\n",
      "\r\n"
     ]
    }
   ],
   "source": [
    "a=\"*\"\n",
    "b=6\n",
    "for i in range(b):\n",
    "    for j in range(i):\n",
    "        print(a,end=\"\")\n",
    "    print(\"\\r\")\n",
    "for i in range(b,0,-1):\n",
    "    for j in range(2,i):\n",
    "        print(a,end=\"\")\n",
    "    print(\"\\r\")"
   ]
